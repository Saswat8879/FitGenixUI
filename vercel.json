{
  "version": 2,
  "buildCommand": "npm run vercel-build",
  "builds": [
    {
      "src": "run.py",
      "use": "@vercel/python"
    }
  ],
  "functions": {
    "run.py": {
      "includeFiles": "migrations/**"
    }
  },
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/app/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "run.py"
    }
  ]
}
